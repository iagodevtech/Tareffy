{
  "name": "tareffy",
  "version": "1.0.0",
  "description": "Sistema moderno de gest√£o de tarefas, projetos e equipes com React, TypeScript e Supabase",
  "type": "module",
  "homepage": "https://seu-usuario.github.io/tareffy",
  "scripts": {
    "dev": "vite",
    "dev:open": "vite --open",
    "build": "tsc && vite build",
    "build:gh-pages": "tsc && vite build --base=/tareffy/",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "deploy:gh-pages": "npm run build:gh-pages && gh-pages -d dist"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "framer-motion": "^10.16.4",
    "react-icons": "^4.11.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "gh-pages": "^6.1.0"
  },
  "keywords": [
    "react",
    "typescript",
    "task-management",
    "project-management",
    "supabase",
    "github-pages",
    "responsive",
    "dashboard"
  ],
  "author": "Iago Dev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/seu-usuario/tareffy.git"
  },
  "bugs": {
    "url": "https://github.com/seu-usuario/tareffy/issues"
  },
  "homepage": "https://seu-usuario.github.io/tareffy"
}
